---
import Navbar from '../components/Navbar.tsx';
import Footer from '../components/Footer.astro';
interface Props {
	title: string;
}

const {title} = Astro.props;


---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
		<link rel="icon" type="image/png" href="./../../public/favicon2.png">
		<link href="/src/styles/output.css" rel="stylesheet"></link>
	</head>
	<body class="flex flex-col min-h-screen p-0">
		
		<Navbar client:load/>
		<main class="grow">
			<slot />
		</main>
		<Footer/>
	</body>
</html>